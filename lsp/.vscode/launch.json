{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--disable-extensions"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/dist/**/*.js"
      ],
      "sourceMaps": true,
      "smartStep": true,
      "env": {
        "VSCODE_GO_IN_TEST": ""
      },
      "cwd": "${workspaceFolder}/extension"
    }
  ]
}
